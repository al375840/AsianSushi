#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/adria/OneDrive/Escritorio/AS/AsianSushi/node_modules/needle/bin/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/AS/AsianSushi/node_modules/needle/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/AS/AsianSushi/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/AS/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/node_modules:/mnt/c/Users/adria/OneDrive/node_modules:/mnt/c/Users/adria/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/AS/AsianSushi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/adria/OneDrive/Escritorio/AS/AsianSushi/node_modules/needle/bin/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/AS/AsianSushi/node_modules/needle/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/AS/AsianSushi/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/AS/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/node_modules:/mnt/c/Users/adria/OneDrive/node_modules:/mnt/c/Users/adria/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/adria/OneDrive/Escritorio/AS/AsianSushi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
